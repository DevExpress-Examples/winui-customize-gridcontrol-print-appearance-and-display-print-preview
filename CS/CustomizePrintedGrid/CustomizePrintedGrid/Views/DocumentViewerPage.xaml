<Page
    x:Class="CustomizePrintedGrid.DocumentViewerPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:dxdv="using:DevExpress.WinUI.DocumentViewer"
    xmlns:local="using:CustomizePrintedGridControl"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}"
    mc:Ignorable="d">

    <Grid RowDefinitions="Auto,*">
        <HyperlinkButton
            HorizontalAlignment="Center"
            Click="HyperlinkButton_Click"
            Content="Close Print Preview" />
        <dxdv:DocumentViewer x:Name="documentViewer" Grid.Row="1" />
    </Grid>
</Page>
